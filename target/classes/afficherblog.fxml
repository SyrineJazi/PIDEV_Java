<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.controler.Afficherblog">
   <children>
      <HBox layoutX="158.0" layoutY="74.0" prefHeight="274.0" prefWidth="414.0">
         <children>
            <Button mnemonicParsing="false" onAction="#Deleteblog" text="Button" />
            <ImageView fitHeight="166.0" fitWidth="211.0" pickOnBounds="true" preserveRatio="true" />
            <TextField fx:id="imageb" editable="false" />
            <VBox prefHeight="322.0" prefWidth="79.0">
               <children>
                  <TextField fx:id="titre" editable="false" />
                  <TextField fx:id="contenu" editable="false" />
               </children>
            </VBox>
         </children>
      </HBox>
      <TableView layoutX="14.0" layoutY="50.0" prefHeight="182.0" prefWidth="379.0" stylesheets="@stylesheet.css">
        <columns>
          <TableColumn fx:id="coid" prefWidth="75.0" text="id" />
          <TableColumn fx:id="coltitre" prefWidth="75.0" text="titre" />
            <TableColumn fx:id="colcontenu" prefWidth="75.0" text="contenu" />
            <TableColumn fx:id="colimage" prefWidth="75.0" text="image" />
            <TableColumn fx:id="colaction" prefWidth="75.0" text="action" />
        </columns>
      </TableView>
      <Button mnemonicParsing="false" text="Button" />
   </children>
   <stylesheets>
      <URL value="@stylesheet.css" />
      <URL value="@stylesheet.css" />
   </stylesheets>
</AnchorPane>
